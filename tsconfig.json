{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "src",
    "src/@types",
    "components/Layout",
    "pages/card/[productId].tsx",
    "Utils",
    "components/Search",
    "components/types/components.types.ts",
    "components/Products/ProductItem",
    "components/Pagination/PageButton.tsx",
    "components/Pagination/Pagination.tsx",
    "components/Products/ProductList.tsx",
    "components/Products/Products.tsx"
, "pages/index.tsx", "styles/sharedStyles.tsx", "components/Products/ProductItem.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
